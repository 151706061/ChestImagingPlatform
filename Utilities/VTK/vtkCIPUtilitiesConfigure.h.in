 * These values should only change when the target compile platform changes.
 */

#cmakedefine BUILD_SHARED_LIBS
#ifndef BUILD_SHARED_LIBS
#define VTKCIP_STATIC
#endif

#if defined(WIN32) && !defined(VTKCIPUtilities_STATIC)
